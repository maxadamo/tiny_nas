{
  "name": "maxadamo-tiny_nas",
  "version": "0.3.0",
  "author": "maxadamo",
  "summary": "Tiny NAS: a Tiny Highly-Avalaible NAS Appliance",
  "license": "GPL-3.0",
  "source": "https://github.com/maxadamo/tiny_nas",
  "project_page": "https://github.com/maxadamo/tiny_nas",
  "issues_url": "https://github.com/maxadamo/tiny_nas/issues",
  "dependencies": [
    {
      "name": "derdanne-nfs",
      "version_requirement": ">= 2.0.8 <= 2.0.8"
    },
    {
      "name": "puppetlabs-firewall",
