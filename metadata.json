{
  "name": "maxadamo-tiny_nas",
  "version": "0.2.0",
  "author": "maxadamo",
  "summary": "Remote directory synchronization with Lscynd and Csync2",
  "license": "GPL-3.0",
  "source": "https://github.com/maxadamo/tiny_nas",
  "project_page": "https://github.com/maxadamo/tiny_nas",
  "issues_url": "https://github.com/maxadamo/tiny_nas/issues",
  "dependencies": [
    {
      "name": "puppetlabs-firewall",
      "version_requirement": ">= 1.9.0"
    },
    {
      "name": "puppetlabs-xinetd",
      "version_requirement": ">= 3.0.0"
    }

  ],
  "data_provider": "hiera",
  "operatingsystem_support": [{
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    }
  ]
}

